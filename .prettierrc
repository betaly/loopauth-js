{
  "bracketSpacing": false,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "semi": true,
  "overrides": [
    {
      "files": "**/*.md",
      "options": {
        "parser": "markdown",
        "proseWrap": "always"
      }
    },
    {
      "files": "**/*.ts",
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
