{
  "name": "@loopauth/browser",
  "description": "LoopAuth Browser client written in TypeScript",
  "version": "0.4.4",
  "keywords": [],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "ty",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "replace '\\d+\\.\\d+\\.\\d+' \\'$(npx json -f package.json version)\\' src/version.ts && git add src/version.ts",
    "build": "nb-tsc && rollup -c",
    "clean": "del 'loopx-auth-browser*.tgz' 'dist/*' '*.tsbuildinfo' 'package'",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn run pretest && yarn run jest",
    "jest": "jest"
  },
  "dependencies": {
    "@loopauth/client": "workspace:^",
    "browser-tabs-lock": "^1.3.0",
    "es-cookie": "1.3.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.19",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "del-cli": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "json": "^11.0.0",
    "replace": "^1.2.2",
    "rollup": "^4.9.6"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
